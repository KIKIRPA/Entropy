{
    "raman": {
        "alias": [
            "Raman",
            "Raman 785nm",
            "Raman 514nm",
            "Raman 532nm",
            "Raman 633nm",
            "Raman 1064nm"
        ],
        "viewer": "xy",
        "graph": {
            "xy": "line"
        },
        "axis": {
            "x": [
                 {
                    "json": "Raman shift (cm-1)",
                    "html": "Raman shift (cm<sup>-1<\/sup>)",
                    "jcampdx": "Raman shift (cm-1)",
                    "other1": "Raman shift (1/cm)",
                    "other2": "Wavenumber (cm-1)",
                    "other3": "Wavenumber (1/cm)"
                }
            ],
            "y": [
                {
                    "json": "Intensity (Arbitrary Units)",
                    "html": "Intensity (Arbitrary Units)",
                    "jcampdx": "Intensity (Arbitrary Units)",
                    "other1": "Counts"
                }
            ]
        }
    },
    "ftir": {
        "alias": [
            "FTIR"
        ],
        "viewer": "xy",
        "graph": {
            "xy": "line"
        },
        "axis": {
            "x": [
                {
                    "json": "Wavenumber (cm-1)",
                    "html": "Wavenumber (cm<sup>-1<\/sup>)",
                    "other1": "Wavenumber (1/cm)",
                    "invert": true
                }
            ],
            "y": [
                {
                   "json": "Absorbance (Relative intensity)",
                    "html": "Absorbance (Relative intensity)"
                },
                {
                    "json": "Transmission (%)",
                    "html": "Transmission (%)",
                    "other1": "Transmission"
                }
            ]
        }
    },
    "ms": {
        "alias": [
            "MS",
            "Mass spectrum"
        ],
        "viewer": "xy",
        "graph": {
            "xy": "bar"
        },
        "axis": {
            "x": [
                {
                    "json": "m/z",
                    "html": "m\/z"
                }
            ],
            "y": [
                {
                    "json": "Relative intensity",
                    "html": "Relative intensity"
                }
            ]
        }
    },
    "chromms": {
        "alias": [
            "ChromMS",
            "GC-MS"
        ],
        "viewer": "txy",
        "graph": {
            "ty": "line",
            "xy": "bar"
        },
        "axis": {
            "t":[
                {
                    "json": "Time (min)",
                    "html": "Time (min)"
                }
            ],
            "x":[
                {
                    "json": "m/z",
                    "html": "m\/z"
                }
            ],
            "y": [
                {
                    "json": "Relative intensity",
                    "html": "Relative intensity"
                }
            ]
        }
